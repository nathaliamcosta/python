segunda = int(input("Segunda-feira: "))
terca = int(input("Terça-feira "))
quarta = int(input("Quarta-feira: "))
quinta = int(input("Quinta-feira: "))
sexta = int(input("Sexta-feira: "))

if segunda > terca:
    print("O dia mais votado foi segunda-feira")

elif terca > segunda:
    print("O dia mais votado foi terça-feira")

elif segunda > quarta:
    print("O dia mais votado foi segunda-feira")

elif quarta < segunda:
    print("O dia mais votado foi quarta-feira")

elif segunda > quinta:
    print("O dia mais votado foi segunda-feira")

elif quinta < segunda:
    print("O dia mais votado foi quintafeira")

elif segunda > sexta:
    print("O dia mais votado foi segunda-feira")

elif sexta > segunda:
    print("O dia mais votado foi sexta-feira")

elif terca > quarta:
    print("O dia mais votado foi terça-feira")

elif quarta > terca:
    print("O dia mais votado foi quarta-feira")


elif terca > quinta:
    print("O dia mais votado foi terça-feira")

elif quinta > terca:
    print("O dia mais votado foi quinta-feira")


elif terca > sexta:
    print("O dia mais votado foi terça-feira")


elif sexta > terca:
    print("O dia mais votado foi sexta-feira")


elif quarta > quinta:
    print("O dia mais votado foi quarta-feira")

elif quinta > quarta:
    print("O dia mais votado foi quinta-feira")


elif quarta > sexta:
    print("O dia mais votado foi quarta-feira")

elif sexta > quarta:
    print("O dia mais votado foi sexta-feira")


elif quinta > sexta:
    print("O dia mais votado foi quinta-feira")

elif sexta > quinta:
    print("O dia mais votado foi sexta-feira")

else:
    print("Tivemos mais de uma dia mais votado, por favor, preencha novamente (: ")

print("fim")
