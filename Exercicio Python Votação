DAYS = ['Segunda-feira', 'Terca-feira', 'Quarta-feira', 'Quita-feira', 'Sexta-feira']

dayWithHighestVotes = dict(label='', votes=0)
duplicated = []
for day in DAYS:
    aux = int(input(day + ": "))
    vote = dict(label=day, votes=aux)
    
    # Verifica se o que foi colocado e o dia com mais votos
    # Se for eu preciso limpar meu array de duplicados, pois
    # vou utilizar isso depois para validar se tem mais de um dia com
    # votos iguais

    if aux > dayWithHighestVotes["votes"]:
        dayWithHighestVotes = vote
        duplicated = [vote]
    
    # Se quantidade votos e igual ao dia mais votado
    # adiciona ao array de duplicados
    elif aux == dayWithHighestVotes["votes"]:
        duplicated.append(vote)

# Como temos o array de duplicados
# podemos printar quais sao os dias com votacao empatada 

if len(duplicated) > 1:
    print "Tivemos mais de uma dia mais votado, por favor, preencha novamente (: "
    print duplicated
else:
    print "O dia com mais votos foi: " + dayWithHighestVotes["label"]
